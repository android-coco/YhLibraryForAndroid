<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.yh.library">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <provider
            android:authorities="${applicationId}.AgentWebFileProvider"
            android:name=".view.webview.AgentWebFileProvider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data android:name="android.support.FILE_PROVIDER_PATHS"
                       android:resource="@xml/web_download_paths"/>
        </provider>
        <!-- 小视频录制Activity -->
        <activity android:name="com.mabeijianxi.smallvideorecord2.MediaRecorderActivity"/>
    </application>

</manifest>
